<title>Insert title here</title>

<%
	Git_print gp = new Git_print();
	Git_sum gs = new Git_sum();
%>

<script>
	$(function(){
		$("#print").click(function(){
			$(this).append("<%=gp.gitPrint()%>");
		})
		
		$("#sum").click(function(){
			$(this).append("<%=gs.gitSum()%>");
		})
	})
	

</script>

</head>
<body>
	<div>
	<h3>테스트용</h3>
	<button id="print">출력</button>
	
	<button id="sum">더하기</button>
	
